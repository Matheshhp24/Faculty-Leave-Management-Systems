{
  "builds": [
    {
      "src": "fms/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "./build.sh",
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "fms/wsgi.py"
    }
  ]
}
