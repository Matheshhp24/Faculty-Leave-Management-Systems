{
    "builds": [{
        "src": "fms/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb","installCommand": "python -m pip install -r requirements.txt", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "fms/wsgi.py"
        }
    ]
}
