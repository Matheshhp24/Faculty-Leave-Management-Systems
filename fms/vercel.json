{
  "builds": [
    {
      "src": "fms/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "./build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "fms/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "VERCEL_BUILD_CACHE": "false"
    }
  }
}
